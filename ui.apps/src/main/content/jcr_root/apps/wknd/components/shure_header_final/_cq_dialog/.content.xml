<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/tabs"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <header_top
                                jcr:primaryType="nt:unstructured"
                                jcr:title="HeaderTop"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <logo
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        fieldLabel="Logo"
                                                        fileNameParameter="./fileName"
                                                        fileReferenceParameter="./fileReference"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        name="./logo"/>
                                                    <mobilemenu
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        fieldDescription="Please upload the svg image here"
                                                        fieldLabel="Upload Mobile Menu Image"
                                                        fileNameParameter="./fileName"
                                                        fileReferenceParameter="./mobileMenu"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        name="./menu"/>
                                                    <search
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <searchbar
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Search Placeholder"
                                                                name="./searchbar"/>
                                                            <signl
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                fieldLabel="Upload Search Button Image"
                                                                fileNameParameter="./fileName"
                                                                fileReferenceParameter="./searchBtn"
                                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                                name="./searchButton"/>
                                                        </items>
                                                    </search>
                                                    <addtocart
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        fieldDescription="Please upload the svg image here"
                                                        fieldLabel="Upload Add to Cart Image"
                                                        fileNameParameter="./fileName"
                                                        fileReferenceParameter="./addToCart"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        name="./addToCartBtn"/>
                                                    <wishlist
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        fieldDescription="Please Upload the wishlist image icon here  "
                                                        fieldLabel="Upload Wishlist Image"
                                                        fileNameParameter="./fileName"
                                                        fileReferenceParameter="./wishlist"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        name="./wishlistBtn"/>
                                                    <signin
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <svgpath
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                fieldDescription="Please upload the sign in image icon here"
                                                                fieldLabel="Upload Sign In Image"
                                                                fileNameParameter="./fileName"
                                                                fileReferenceParameter="./signInPath"
                                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                                name="./signInBtn"/>
                                                            <signl
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Sign In Button Label"
                                                                name="./signIn"/>
                                                        </items>
                                                    </signin>
                                                    <mobilemenuclose
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        fieldDescription="Please upload the svg image here"
                                                        fieldLabel="Upload Menu Close Image"
                                                        fileNameParameter="./fileName"
                                                        fileReferenceParameter="./mobileMenuClose"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        name="./menuCloseBtn"/>
                                                    <searchmobile
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        fieldDescription="Please upload the svg image here"
                                                        fieldLabel="Upload Mobile Search Icon Image"
                                                        fileNameParameter="./fileName"
                                                        fileReferenceParameter="./mobileSearchBtn"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        name="./mobileSearchButton"/>
                                                    <mobile_sidebar_image
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        fieldLabel="Mobile View Shure Image"
                                                        fileNameParameter="./fileName"
                                                        fileReferenceParameter="./mobileSideShureImg"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        name="./mobileShureImg"/>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </header_top>
                            <header_bottom
                                jcr:primaryType="nt:unstructured"
                                jcr:title="HeaderBottom"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <multi
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        composite="{Boolean}true"
                                                        fieldLabel="Products">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            name="./field1">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <prod
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Product"
                                                                    name="./prod"/>
                                                                <link
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Link To Products"
                                                                    name="./link"/>
                                                                <multi2
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                    composite="{Boolean}true">
                                                                    <field
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                        name="./field2">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <item
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldLabel="ProdItem"
                                                                                name="./prodItem"/>
                                                                            <itemlink
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldLabel="Item Link"
                                                                                name="./prodLink"/>
                                                                        </items>
                                                                    </field>
                                                                </multi2>
                                                            </items>
                                                        </field>
                                                    </multi>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </header_bottom>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
