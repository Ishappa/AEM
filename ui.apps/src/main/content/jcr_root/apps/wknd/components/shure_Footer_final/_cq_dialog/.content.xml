<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Deatils"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <Product_List_Tab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Shopping and Visit"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        name="./sub">
                        <items jcr:primaryType="nt:unstructured">
                            <multifield
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./fields1">
                                    <items jcr:primaryType="nt:unstructured">
                                        <heading
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Title"
                                            name="./title"/>
                                        <multifieldNested
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                            composite="{Boolean}true"
                                            fieldLabel="List of Products">
                                            <field
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                name="./fields2">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <List_Items
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="List_Item - Name"
                                                        name="./prod"/>
                                                    <path
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Path"
                                                        name="./path1"/>
                                                </items>
                                            </field>
                                        </multifieldNested>
                                    </items>
                                </field>
                            </multifield>
                        </items>
                    </Product_List_Tab>
                    <ContactUs_Tab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Contact-Us"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        name="./contact">
                        <items jcr:primaryType="nt:unstructured">
                            <contact
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Title for Contact"
                                name="./contact"/>
                            <path
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Path"
                                name="./path"/>
                            <email
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Email Id"
                                name="./email"/>
                        </items>
                    </ContactUs_Tab>
                    <Subscribe_Tab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Subscribe Content"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        name="./list">
                        <items jcr:primaryType="nt:unstructured">
                            <subscribeTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Subscribe Heading"
                                name="./stitle"/>
                            <mail_icon
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                fieldLabel="Upload mail logo here"
                                fileReferenceParameter="./mailIcon"
                                mimeTypes="image/gif,image/png,image/jpeg"
                                name="./file"/>
                            <mailPath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Mail Path"
                                name="./mailPath"/>
                            <multifield_Icon
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldLabel="Social Links">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./fields3">
                                    <items jcr:primaryType="nt:unstructured">
                                        <logo
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                            fileReferenceParameter="./fileReference"
                                            mimeTypes="image/gif,image/png,image/jpeg"
                                            name="./file"
                                            title="Upload Image Asset"/>
                                        <path
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Path"
                                            name="./path"/>
                                        <iconName
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Name of Icon(Lower Case)"
                                            name="./iname"/>
                                    </items>
                                </field>
                            </multifield_Icon>
                        </items>
                    </Subscribe_Tab>
                    <Final_copy_right
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Copy Rigth Section"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        name="./final">
                        <items jcr:primaryType="nt:unstructured">
                            <logo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                fieldLabel="Upload logo here"
                                fileReferenceParameter="./fileReference"
                                mimeTypes="image/gif,image/png,image/jpeg"
                                name="./file"/>
                            <copyright
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="CopyRight"
                                name="./cr"/>
                        </items>
                    </Final_copy_right>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
